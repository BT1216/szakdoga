{
    "env": {
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:n/recommended"],
    "plugins": [
        "eslint-plugin-import",
        "eslint-plugin-sonarjs",
        "eslint-plugin-unicorn"
    ],
    "rules": {
        "no-console": ["error", { "allow": ["info", "error"] }],
        "n/no-process-exit": "0"
    },
    "root": false
}